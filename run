#!/usr/bin/env nu
 
def "main web" [] {
  let server_port = (port 8080);
  cd ours
  cargo run -- ~/ $server_port
}

def "main desk" [] {
  cd ours/
  cargo run
}

def "main build" [] {
  cd ours
  cargo build --release
}

def "main install" [] {
  main build 
  cargo install --path ./ours
}

def main [] {}
